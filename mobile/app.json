    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon00.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "alisson.copadomundo"

    },
    "web": {
      "favicon": "./assets/favicon00.png"
    }
  }
}