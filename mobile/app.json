{
  "expo": {
    "name": "copadomundo",
    "slug": "copadomundo",
    "scheme": "copadomundo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon00.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash00.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
